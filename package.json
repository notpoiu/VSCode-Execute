{
  "name": "vscode-execute",
  "displayName": "VSCode Execute",
  "description": "Execute the current contents of the file to Roblox, an executor is required for this to work",
  "version": "0.0.5",
  "publisher": "ActualMasterOogway",
  "repository": {
    "url": "https://github.com/ActualMasterOogway/VSCode-Execute"
  },
  "engines": {
    "vscode": "^1.56.0",
    "vscode-insiders": "^1.56.0"
  },
  "activationEvents": [
    "onCommand:vsexecute.executeCurrentFile",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./out/install.js"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/localtunnel": "^2.0.4",
    "@types/node": "^14.14.37",
    "@types/vscode": "^1.56.0",
    "typescript": "^4.0.0",
    "vscode-test": "^1.4.0"
  },
  "keywords": [
    "roblox execute",
    "script ware",
    "scripware",
    "vscode execute",
    "synapse",
    "roblox",
    "krnl",
    "krampus",
    "roexec",
    "ro-exec",
    "oogway bytes",
    "execute",
    "delta",
    "codex",
    "hydrogen",
    "macsploit",
    "valkyrie"
  ],
  "dependencies": {
    "axios": "^1.7.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1"
  },
  "contributes": {
    "commands": [
      {
        "command": "vsexecute.executeCurrentFile",
        "title": "Execute Current Opened File"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "VSCode Execute",
      "properties": {
        "vscode-execute.useSpecificFile": {
          "type": "boolean",
          "default": false,
          "description": "option to use a specific file instead of the current file"
        },
        "vscode-execute.specificFile": {
          "type": "string",
          "default": "",
          "description": "the specific file to use if 'use-specific-file' is enabled (from the root of the workspace)"
        }
      }
    }
  }
}
